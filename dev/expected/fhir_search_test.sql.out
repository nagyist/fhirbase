WITH previous AS (
  SELECT
    alert.alert#>>'{entry,0,id}' AS id,
    alert.alert#>>'{entry,0,link,0,href}' AS vid
  FROM fhir_create('{"base":"https://test.me"}', 'Alert', '{
  "resourceType": "Alert",
   "status": "active",
   "subject": {
     "reference": "Patient/example",
     "display": "Peter Patient"
   },
   "note": "previous-note"
}'::jsonb, '[]'::jsonb) alert
), updated AS (
  SELECT
    p.id,
    p.vid,
    _extract_vid(
      fhir_update('{"base":"https://test.me"}', 'Alert', p.id, p.vid, '{
  "resourceType": "Alert",
   "status": "active",
   "subject": {
     "reference": "Patient/example",
     "display": "Peter Patient"
   },
   "note": "previous-note"
}'::jsonb, '[]'::jsonb)
    #>>'{entry,0,link,0,href}') AS new_vid
  FROM previous p
)
SELECT assert_raise(
  'Wrong version_id ' || _extract_vid(u.vid) || '. Current is ' || _extract_vid(u.new_vid),
  'SELECT fhir_update(''' || '{"base":"https://test.me"}' || ''', ''Alert'', ''' || u.id || ''', ''' || u.vid || ''', ''' || '{
  "resourceType": "Alert",
   "status": "active",
   "subject": {
     "reference": "Patient/example",
     "display": "Peter Patient"
   },
   "note": "previous-note"
}' || ''', ''[]''::jsonb)',
  'update with no current version_id')
FROM updated u;
